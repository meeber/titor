  },
  "peerDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.10.2",
    "exorcist": "^0.4.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.5.3",
    "semver": "^5.1.0"
  },
  "bin": {
    "titor": "./bin/titor.js"
  },
  "scripts": {
    "clean": "rm -R -f coverage/",
    "cover": "npm run clean && istanbul cover --report lcovonly _mocha -- -t 5000 -r test/fixture 'test/**/*.test.js'",
    "cover-report": "npm run cover && cat coverage/lcov.info | coveralls",
    "lint": "eslint --fix .",
    "postversion": "git checkout dev && git merge master && git push && git push --tags && npm publish",
    "preversion": "git checkout master && git merge dev && npm test",
    "release": "npm version -m 'Finalize v%s'",
    "test": "mocha -t 5000 -r test/fixture 'test/**/*.test.js' && npm run lint",
    "travis": "npm run lint && npm run cover-report"
  }
}
