{
  "name": "titor",
  "version": "0.0.0",
  "description": "package assistant",
  "author": "Grant Snodgrass <meeber@gametypething.com>",
  "license": "MIT",
  "repository": "meeber/titor",
  "main": "index.js",
  "engines": {
    "node": ">=0.12.0"
  },
  "scripts": {
    "lint": "eslint --fix .",
    "test": "npm run lint && npm run test-src",
    "test-dev": "npm run test-src-unit && npm run lint",
    "test-src": "npm run test-src-unit && npm run test-src-integration",
    "test-src-integration": "BABEL_ENV=current mocha -r babel-core/register src/test/bootstrap/integration",
    "test-src-unit": "BABEL_ENV=current mocha -r babel-core/register src/test/bootstrap/unit"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.0.5",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.13.1",
    "eslint-config-tt": "^3.4.0",
    "eslint-plugin-babel": "^3.2.0",
    "mocha": "^2.5.3",
    "proxyquire": "^1.7.9",
    "shelljs": "^0.7.0",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "require-dir": "^0.3.0"
  }
}
